{
  "name": "nickdnk/gatewayapi-php",
  "description": "PHP library based on Guzzle for integration with gatewayapi.com",
  "type": "library",
  "require": {
    "php": "^7.1 || ^8.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.0 || ^7.0",
    "guzzlehttp/oauth-subscriber": "0.3.* || 0.4.* || 0.5.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0 || ^8.0 || ^9.0",
    "php-coveralls/php-coveralls": "^2.2"
  },
  "license": "mit",
  "authors": [
    {
      "name": "Nicolai Cornelis",
      "email": "nickdnk@hotmail.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "nickdnk\\GatewayAPI\\": "src/"
    },
    "exclude-from-classmap": [
      "/tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "nickdnk\\GatewayAPI\\Tests\\": "tests/"
    }
  }
}
